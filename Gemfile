  gem "jekyll-last-modified-at"
  gem "jekyll-sitemap"
  gem "jekyll-target-blank"
  gem "jekyll-tailwindcss"
  gem "tailwindcss-ruby"
  gem "jekyll-tidy"
  gem 'tzinfo'
